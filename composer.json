{
    "name": "dorcas/modules-ecommerce",
    "description": "Dorcas Hub eCommerce Module",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ifeoluwa Olawoye",
            "email": "bolaji.olawoye@hostvillenigeria.org"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1|^8.2",
        "aws/aws-sdk-php": "^3.74",
        "guzzlehttp/guzzle": "^7.4.5",
        "laravel/framework": "^8.0|^10.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dorcas\\ModulesEcommerce\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorcas\\ModulesEcommerce\\ModulesEcommerceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
